<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- 拷贝以下内容到struts.xml -->
<!-- 
	<include file="struts/vote-struts.xml"></include>
-->
<struts>
	<package name="vote" extends="default">
		<action name="voteAction_*" class="com.weichat.boss.action.vote.VoteAction" method="{1}">
			<result name="list">/WEB-INF/jsp/vote/vote_list.jsp</result>
			<result name="add">/WEB-INF/jsp/vote/vote_add.jsp</result>
			<result name="edit">/WEB-INF/jsp/vote/vote_edit.jsp</result>
			<result name="view">/WEB-INF/jsp/vote/vote_view.jsp</result>
		</action>
	</package>
</struts>