<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- 拷贝以下内容到struts.xml -->
<!-- 
	<include file="struts/channel-struts.xml"></include>
-->
<struts>
	<package name="channel" extends="struts-default">
		<action name="channelAction_*" class="com.weichat.boss.action.channel.ChannelAction" method="{1}">
			<result name="list">/WEB-INF/jsp/channel/channel_list.jsp</result>
			<result name="edit">/WEB-INF/jsp/channel/channel_edit.jsp</result>
			<result name="view">/WEB-INF/jsp/channel/channel_view.jsp</result>
			<result name="add">/WEB-INF/jsp/channel/channel_add.jsp</result>
		</action>
	</package>
</struts>