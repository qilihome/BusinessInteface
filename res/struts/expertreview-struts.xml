	<include file="struts/expertreview-struts.xml"></include>
-->
<struts>
	<package name="expertreview" extends="default">
		<action name="expertReviewAction_*" class="com.weichat.boss.action.expertreview.ExpertReviewAction" method="{1}">
			<result name="list">/WEB-INF/jsp/expertreview/expertreview_list.jsp</result>
			<result name="add">/WEB-INF/jsp/expertreview/expertreview_add.jsp</result>
			<result name="edit">/WEB-INF/jsp/expertreview/expertreview_edit.jsp</result>
			<result name="view">/WEB-INF/jsp/expertreview/expertreview_view.jsp</result>
		</action>
	</package>
</struts>