<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- 拷贝以下内容到struts.xml -->
<!-- 
	<include file="struts/albumphoto-struts.xml"></include>
-->
<struts>
	<package name="albumphoto" extends="default">
		<action name="albumphotoAction_*" class="com.weichat.boss.action.albumphoto.AlbumphotoAction" method="{1}">
			<result name="list">/WEB-INF/jsp/albumphoto/albumphoto_list.jsp</result>
			<result name="edit">/WEB-INF/jsp/albumphoto/albumphoto_edit.jsp</result>
			<result name="view">/WEB-INF/jsp/albumphoto/albumphoto_view.jsp</result>
			<result name="add">/WEB-INF/jsp/albumphoto/albumphoto_add.jsp</result>
		</action>
	</package>
</struts>