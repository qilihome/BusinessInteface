<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- 拷贝以下内容到struts.xml -->
<!-- 
	<include file="struts/module-struts.xml"></include>
-->
<struts>
	<package name="module" extends="struts-default">
		<action name="moduleAction_*" class="com.weichat.boss.action.module.ModuleAction" method="{1}">
			<result name="list">/WEB-INF/jsp/module/module_list.jsp</result>
			<result name="edit">/WEB-INF/jsp/module/module_edit.jsp</result>
			<result name="view">/WEB-INF/jsp/module/module_view.jsp</result>
			<result name="add">/WEB-INF/jsp/module/module_add.jsp</result>
		</action>
	</package>
</struts>