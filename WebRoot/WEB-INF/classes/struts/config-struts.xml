<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- 拷贝以下内容到struts.xml -->
<!-- 
	<include file="struts/config-struts.xml"></include>
-->
<struts>
	<package name="config" extends="default">
		<action name="configAction_*" class="com.weichat.boss.action.config.ConfigAction" method="{1}">
			<result name="config">/WEB-INF/jsp/config/template_config.jsp</result>
			<result name="config_result">/WEB-INF/jsp/config/config_result.jsp</result>
		</action>
	</package>
</struts>