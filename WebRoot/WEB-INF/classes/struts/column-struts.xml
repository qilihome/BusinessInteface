<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- 拷贝以下内容到struts.xml -->
<!-- 
	<include file="struts/column-struts.xml"></include>
-->
<struts>
	<package name="column" extends="struts-default">
		<action name="columnAction_*" class="com.weichat.boss.action.column.ColumnAction" method="{1}">
			<result name="list">/WEB-INF/jsp/column/column_list.jsp</result>
			<result name="edit">/WEB-INF/jsp/column/column_edit.jsp</result>
			<result name="view">/WEB-INF/jsp/column/column_view.jsp</result>
			<result name="add">/WEB-INF/jsp/column/column_add.jsp</result>
		</action>
	</package>
</struts>